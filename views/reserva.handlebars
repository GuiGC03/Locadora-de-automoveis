<div id="reserva-page">
  <a href="/reservas">Voltar</a>
  
  <div class="container">
    <p>Modelo: {{reserva.idcliente}}</p>
    <p>Ano: {{reserva.idveiculo}}</p>
    <p>Data inicial: {{reserva.datainicio}}</p>
    <p>Data final: {{reserva.datafim}}</p>
    <div class="actions">
      <a href="/reservas/edit/{{reserva.id}}">Editar</a>
      <form action="/reservas/remove/{{reserva.id}}" method="POST">
      <input type="submit" value="Remover">
      </form>
    </div>
  </div>
</div>