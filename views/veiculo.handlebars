<div id="veiculo-page">
  <a href="/veiculos">Voltar</a>
  
  <div class="container">
    <h1>Marca: {{veiculo.marca}}</h1>
    <p>Modelo: {{veiculo.modelo}}</p>
    <p>Ano: {{veiculo.ano}}</p>
    <p>Placa: {{veiculo.placa}}</p>
    <p>Disponibilidade: {{veiculo.disponibilidade}}</p>
    <div class="actions">
      <a href="/veiculos/edit/{{veiculo.id}}">Editar</a>
      <form action="/veiculos/remove/{{veiculo.id}}" method="POST">
      <input type="submit" value="Remover">
      </form>
    </div>
  </div>
</div>